{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "lib": [
            "es6"
        ],
        "noImplicitAny": false,
        "noImplicitThis": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "alwaysStrict": true,
        "removeComments": true,
        "outDir": "built/",
        "sourceMap": true,
        "declaration": true
    },
    "exclude": [
        "node_modules",
        "built/",
        "decl/"
    ]
}
